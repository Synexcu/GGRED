---
import BaseLayoutGGBLU from "@/layouts/BaseLayoutGGBLU.astro"
import { Button, buttonVariants } from "@/components/ui/button"
import MaxWidthWrapper from "@/components/MaxWidthWrapper.astro"
import ClanEmblem from "@/components/ClanEmblemConstr.astro"
import { Transition } from '@headlessui/react'

// import Image from "astro/components/Image.astro";
import { Image } from "astro:assets";

import { FaDiscord } from "react-icons/fa";

import { fetchClanInfo } from '@/pages/api/clanReq.ts';
import { fetchClanWebCBInfo } from '@/pages/api/clanWebReq.ts'

let GGRED = await fetchClanInfo("asia", 1);
let GGBLU = await fetchClanInfo("asia", 2);
let CIMA = await fetchClanInfo("na", 3);

let GGREDWebCBInfo = await fetchClanWebCBInfo(1);
let GGBLUWebCBInfo = await fetchClanWebCBInfo(2);

let GGREDiconColor = "#Ef4444"
let GGBLUiconColor = "#3b82f6"

let GGREDtextColor = "red"
let GGBLUtextColor = "blue"

let clanIconColor = GGBLUiconColor // Change this
let clanTextColor = GGBLUtextColor // Change this

let leagueColor : any
let leagueTitle : string
let leagueHex : any
function leagueTranslator(league : number) {
  if (league === 4) {
    leagueColor = "text-[#CC9966]"
    leagueTitle = "Squall"
    leagueHex = "#CC9966"
  } else if (league === 3) {
    leagueColor = "text-[#CCE4E4]"
    leagueTitle = "Gale"
    leagueHex = "#CCE4E4"
  } else if (league === 2) {
    leagueColor = "text-[#E3D6A0]"
    leagueTitle ="Storm"
    leagueHex = "#E3D6A0"
  } else if (league === 1) {
    leagueColor = "text-[#BEE7BD]"
    leagueTitle ="Typhoon"
    leagueHex = "#BEE7BD"
  } else if (league === 0) {
    leagueColor = "text-[#CDA4FF]"
    leagueTitle ="Hurricane"
    leagueHex = "#CDA4FF"
  }
  return {leagueColor, leagueTitle, leagueHex}
}

function divisionTranslator(division : number) {
  if (division === 1) {
    return "I"
  } else if (division === 2) {
    return "II"
  } else if (division === 3) {
    return "III"
  }
} 

const filteredMembersExec = Object.entries(GGBLU!.members).filter(
    ([id, member]) => member.role === 'executive_officer'
);

const filteredMembersRecruit = Object.entries(GGBLU!.members).filter(
    ([id, member]) => member.role === 'recruitment_officer'
);

const filteredMembersCommOfficer = Object.entries(GGBLU!.members).filter(
    ([id, member]) => member.role === 'commissioned_officer'
);

const filteredMembersOfficer = Object.entries(GGBLU!.members).filter(
    ([id, member]) => member.role === 'officer'
);

const filteredMembersMember = Object.entries(GGBLU!.members).filter(
    ([id, member]) => member.role === 'private'
);

const leagueInfo = leagueTranslator(GGBLUWebCBInfo!.max_position.league)
